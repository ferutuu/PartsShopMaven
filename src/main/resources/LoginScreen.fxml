<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.ferutuu.partsshop.LoginController">
    <children>
        <TextField fx:id="usernameField" layoutX="100.0" layoutY="100.0" />
        <PasswordField fx:id="passwordField" layoutX="100.0" layoutY="150.0" />
        <Button layoutX="100.0" layoutY="200.0" text="Login" onAction="#handleLoginButtonAction" />
        <Button layoutX="200.0" layoutY="200.0" text="Register" onAction="#handleRegisterButtonAction" />
    </children>
</AnchorPane>